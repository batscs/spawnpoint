tr
    td.no-mobile
        img(src="/static/resources/projects/" + project.banner)
    td
        a.name(href="/admin/project/" + project.id)=project.name + "(" + project.views + ")"
    td.no-mobile
        if project.isPublished
            a Public
        else
            a Private
    td.no-mobile
        a=project.startDate
    td.no-mobile
        a=project.endDate
    td
        div.options
            a(href="/admin/project/" + project.id) ✏️️
            a(href="/project/" + project.id, target="_blank") 📖
            a(href="/admin/delete/" + project.id) 🗑️
